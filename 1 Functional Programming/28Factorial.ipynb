    "# FACTORIAL OF A NUMBER\n",
    "\n",
    "def fact(n):\n",
    "    f = 1\n",
    "    for i in range(1,n+1):\n",
    "        f = f*i\n",
    "    return f\n",
    "x = 3\n",
    "result = fact(x)\n",
    "print(result)"
   ]
  }
