    "# SIMPLE PROGRAM OF FACTORIAL USING RECURSION\n",
    "\n",
    "def fact(n):\n",
    "    if n == 0: return 1\n",
    "    return n * fact(n-1)\n",
    "result = fact(5)\n",
    "print(result)"
   ]
